{
  "name": "ng4-auto-complete",
  "version": "1.0.0",
  "description": "An Auto-Complete Module for Angular 4 with extended features and make life easy.",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/autoComplete.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/autoComplete.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hardy12994/ng4-auto-complete.git"
  },
  "keywords": [
    "ng4-auto-complete",
    "auto-complete",
    "ng4",
    "auto-list-ng4",
    "auto-input-ng4",
    "ng4-auto-input",
    "ng-auto-complete",
    "autoComplete-ng4"
  ],
  "author": "hardeepsingh12994@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hardy12994/ng4-auto-complete/issues"
  },
  "homepage": "https://github.com/hardy12994/ng4-auto-complete#readme",
  "dependencies": {
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.0",
    "@angular/compiler": "^4.0.0",    
    "node-sass": "^4.5.3",
    "rollup": "^0.50.1",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-typescript2": "^0.8.0",
    "typescript": "^2.3.4",
    "uglify-js": "^3.3.12"
  }
}